{
    "$schema": "https://json.schemastore.org/prettierrc",
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "bracketSameLine": false,
    "endOfLine": "lf",
    "embeddedLanguageFormatting": "auto",
    "proseWrap": "preserve"
}
